{
  "name": "nuxt3-payload-template",
  "private": true,
  "engine": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ."
  },
  "husky": {
